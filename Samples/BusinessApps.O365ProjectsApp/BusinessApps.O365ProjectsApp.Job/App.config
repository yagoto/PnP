<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="O365ProjectsConfiguration" type="BusinessApps.O365ProjectsApp.Infrastructure.Configuration.O365ProjectsConfigurationSectionHandler, BusinessApps.O365ProjectsApp.Infrastructure" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <connectionStrings>
    <add name="AzureWebJobsDashboard" connectionString="DefaultEndpointsProtocol=https;AccountName=[AccountName];AccountKey=[AccountKey]" />
    <add name="AzureWebJobsStorage" connectionString="DefaultEndpointsProtocol=https;AccountName=[AccountName];AccountKey=[AccountKey]" />
  </connectionStrings>

  <appSettings>
    <add key="ida:ClientId" value="[ClientID]" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/" />
    <add key="ida:Domain" value="[Domain]" />
    <add key="ida:TenantId" value="[TenantId]" />
    <add key="ida:PostLogoutRedirectUri" value="https://localhost:44304/" />
    <add key="ida:ClientSecret" value="[ClientSecret]" />
  </appSettings>

  <O365ProjectsConfiguration xmlns="http://schemas.programming-office365.com/Office365/Projects/Configuration">
    <CertificateSettings storeName="My" storeLocation="CurrentUser" />
    <GeneralSettings libraryTitle="BusinessProjects" defaultSiteUrl="[DefaultSiteUrl]" appOnlyCertificateThumbprint="[Thumbprint]" />
  </O365ProjectsConfiguration>

